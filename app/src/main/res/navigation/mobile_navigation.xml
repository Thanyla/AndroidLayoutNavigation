<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_bolo">

    <fragment
        android:id="@+id/nav_bolo"
        android:name="com.example.applayoutnavigation.ui.home.HomeFragment"
        android:label="@string/bolo"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_biscoito"
        android:name="com.example.applayoutnavigation.ui.gallery.GalleryFragment"
        android:label="@string/cookie"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_maca"
        android:name="com.example.applayoutnavigation.ui.slideshow.SlideshowFragment"
        android:label="@string/maca"
        tools:layout="@layout/fragment_slideshow" />

</navigation>